class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int n = nums.size();
        int sum = 0;
        int count = 0;
        unordered_map<int, int> pre_sum;
        pre_sum[0] = 1;
        for(auto i : nums) {
            sum += i;
            if(pre_sum.find(sum-k)!=pre_sum.end()){
                count += pre_sum[sum-k];
            }
            pre_sum[sum]++;
        }
        return count;
    }
};
