class Solution:
    def dividePlayers(self, skill: List[int]) -> int:
        n = len(skill)
        skill.sort()
        ans = 0
        i = 0
        j = n-1
        sum = skill[i] + skill[j]

        while i < j:
            if sum == skill[i] + skill[j]:
                ans += skill[i] * skill[j]
            else:
                return -1
            i += 1
            j -= 1
            
        return ans
